[branch]
	sort = -committerdate
[color "branch"]
	current = magenta
	local = default
	remote = yellow
	upstream = green
	plain = blue
[color "decorate"]
	HEAD = red
	branch = blue
	tag = yellow
	remoteBranch = magenta
[diff]
	algorithm = histogram
[init]
	defaultBranch = main
[interactive]
	singleKey = true
[log]
	abbrevCommit = true
	graphColors = blue,yellow,cyan,magenta,green,red
[page]
	branch = false
	tag = false
[pull]
	default = current
	rebase = true
[rebase]
	autoStash = true
	missingCommitsCheck = warn
[status]
	branch = true
	showStash = true
	showUntrackedFiles = all
[tag]
	sort = -taggerdate
[user]
	name = Gerlito Chagas
	email = gerlito.chagas@gmail.com